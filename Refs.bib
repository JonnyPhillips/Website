@article{Gal2002,
abstract = {The role of occupational welfare, and in particular that of occupational$\backslash$npensions, has gained growing attention in the study of welfare states$\backslash$nin recent years. This article draws on a variety of primary and secondary$\backslash$ndata and two theoretical models, power resources and path dependency,$\backslash$nin order to analyze the emergence of occupational welfare in the$\backslash$nIsraeli welfare state. In particular, it explores the notion of "critical$\backslash$njunctures" and their role in path dependent policies. The article$\backslash$nseeks to understand why the role of occupational welfare in Israel$\backslash$nis so formidable and to examine the implications of this. Its focus$\backslash$nis upon the crucial role that the labor movement played in the formation$\backslash$nof this system during a critical juncture in the evolution of the$\backslash$nIsraeli welfare state. During the 1950s, the Israeli labor movement$\backslash$nbrought about the establishment of an extensive system of occupational$\backslash$nwelfare and a social security system for the elderly based primarily$\backslash$nupon low universal state benefits and generous voluntary occupational$\backslash$nbenefits. The legacy of these systems is still very prevalent in$\backslash$nthe contemporary welfare state and has marked implications both for$\backslash$nspending on social spending and the welfare of the elderly.},
author = {Gal, John and Bargal, David},
doi = {10.1525/sp.2002.49.3.432},
file = {:C$\backslash$:/Users/Jonny/Google Drive/Academic/Harvard/Dissertation/References/Methods/Process Tracing/Gal and Bargal 2002 Israel Labour Path Dependence.pdf:pdf},
issn = {00377791},
