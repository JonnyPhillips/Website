pages = {8--10},
title = {{Y S Rajasekhara Reddy: A Political Appraisal}},
url = {http://www.epw.in/commentary/y-s-rajasekhara-reddy-political-appraisal.html},
volume = {44},
year = {2009}
}
@article{Srivastava2010,
abstract = {Use of indifference curves, defined as functionality between development index and pollution load to evaluate environmental impact, is proposed. Existing Battelle environmental evaluation methodology is subjective in its approach. The use of indifference curves lends a more objective approach to environment assessment methodology. The extent of environmental damage we are prepared to accept, for a development, can be explained by Willingness to Pay and Willingness to Accept approach. The application of proposed approach has been demonstrated taking an example of Power plant set up in forest area. The curves clearly show that cost of EMP considering mitigation of ecological damage is higher than the EMP mitigating impacts of air and water pollution only. The example stresses the need for willingness to accept along with willingness to pay.},
author = {Srivastava, Anjali and Ganguly, Arpita},
doi = {10.1007/s10661-008-0715-6},
issn = {1573-2959},
journal = {Environmental monitoring and assessment},
keywords = {Environment,Environmental Monitoring,Environmental Monitoring: methods,Models,Theoretical},
month = {jan},
number = {1-4},
pages = {513--519},
pmid = {19184494},
title = {{Indifference curves as a tool for environment impact assessment.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21570638},
volume = {160},
year = {2010}
}
