volume = {57},
year = {2013}
}
@unpublished{DeMagalhaes2012,
abstract = {I use a regression discontinuity design to study incumbency effects in Brazilian mayoral elections. For$\backslash$nmayors elected in 1996 I find no evidence of an incumbency effect on the probability of being$\backslash$nelected in 2000. For the 2000-2004 electoral cycle I also find no effect except for races where the$\backslash$nmayor elected in 2000 belonged to the a party in the center-right coalition and the runner-up$\backslash$nbelonged to a party in the center-left coalition. In these races I find an incumbency disadvantage. For$\backslash$nmayors elected in 2004 I find a strong incumbency advantage in the 2008 election across all races. I$\backslash$nalso show some novel incumbency effects. Winning a mayoral election does not have a positive$\backslash$neffect on the future prospects of a politician's career at the state, national or local level. Losing a$\backslash$nmayoral election increases the probability of a politician switching parties.},
