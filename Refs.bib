@article{Gadenne2014,
abstract = {Existing evidence suggests that extra grant revenues lead to little improvements in public services in developing countries - but would governments spend tax revenues differently? This paper considers a program that invests in the tax capacity of Brazilian municipalities. Using variations in the timing of program uptake I find that it raises local tax revenues and that the increase in taxes is used to improve both the quantity and quality of municipal education infrastructure. In contrast increases in grants over which municipalities have the same discretion as over taxes have no impact on any measure of local public infrastructure.},
