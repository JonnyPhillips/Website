@article{Borges2010,
abstract = {Este trabalho procura fornecer elementos para o entendimento das recentes mudan{\c{c}}as verificadas no cen{\'{a}}rio eleitoral dos estados mais pobres e de menor competitividade eleitoral do pa{\'{i}}s. O trabalho tem como objetivo identificar os fatores por tr{\'{a}}s da s{\'{e}}rie de vit{\'{o}}rias obtidas por coaliz{\~{o}}es de centro-esquerda nas elei{\c{c}}{\~{o}}es estaduais de 2002 e 2006, em paralelo ao enfraquecimento dos antigos chefes pol{\'{i}}ticos. Duas hip{\'{o}}teses s{\~{a}}o elaboradas para dar conta dessas transforma{\c{c}}{\~{o}}es. Primeiro, argumenta-se que, no federalismo multipartid{\'{a}}rio constru{\'{i}}do no Brasil, a competi{\c{c}}{\~{a}}o vertical entre os governos federal e estadual na oferta de pol{\'{i}}ticas sociais atua no sentido de constranger as estrat{\'{e}}gias de distribui{\c{c}}{\~{a}}o de patronagem e controle da arena eleitoral dos chefes pol{\'{i}}ticos estaduais. Em segundo lugar, o artigo argumenta que o sistema eleitoral brasileiro tende a promover a fragmenta{\c{c}}{\~{a}}o pol{\'{i}}tica e a aumentar os custos de forma{\c{c}}{\~{a}}o de coaliz{\~{o}}es vencedoras, ao mesmo tempo em que a aus{\^{e}}ncia de partidos pol{\'{i}}ticos fortes dificulta a estabiliza{\c{c}}{\~{a}}o da competi{\c{c}}{\~{a}}o inter-elite e das redes de patronagem controladas pelos governos estaduais. A an{\'{a}}lise emp{\'{i}}rica ap{\'{o}}ia-se em t{\'{e}}cnicas de an{\'{a}}lise de regress{\~{a}}o linear e correla{\c{c}}{\~{a}}o linear simples, de modo a explorar a conex{\~{a}}o entre a pol{\'{i}}tica nacional e estadual ao longo dos anos 1990 e 2000. Por fim, o artigo analisa em detalhe o decl{\'{i}}nio da m{\'{a}}quina pol{\'{i}}tica do Partido da Frente Liberal (PFL) na Bahia, que havia dominado a pol{\'{i}}tica estadual por d{\'{e}}cadas, no contexto da vit{\'{o}}ria do Partido dos Trabalhadores (PT) nas elei{\c{c}}{\~{o}}es para Governador em 2006. Essa se{\c{c}}{\~{a}}o desenvolve um modelo de regress{\~{a}}o linear para testar o impacto da din{\^{a}}mica pol{\'{i}}tica local e dos gastos federais de combate {\`{a}} pobreza (Bolsa Fam{\'{i}}lia) sobre o desempenho eleitoral do PT em 2006. O trabalho conclui que as institui{\c{c}}{\~{o}}es democr{\'{a}}ticas brasileiras conspiram contra a sobreviv{\^{e}}ncia da chefia pol{\'{i}}tica no n{\'{i}}vel estadual, o que indica a necessidade de reavaliar as vis{\~{o}}es j{\'{a}} estabelecidas sobre a pol{\'{i}}tica estadual.},
author = {Borges, Andr{\'{e}}},
doi = {10.1590/S0104-44782010000100011},
file = {:C$\backslash$:/Users/Jonny/Google Drive/Academic/Harvard/Dissertation/References/Brazil/Bahia/Borges 2010 Clientelism Bahia.pdf:pdf},
issn = {0104-4478},
journal = {Revista de Sociologia e Pol{\'{i}}tica},
keywords = {clientelismo,elei{\c{c}}{\~{o}}es estaduais,federalismo,pol{\'{i}}tica social},
number = {35},
pages = {167--188},
title = {{J{\'{a}} n{\~{a}}o se fazem mais m{\'{a}}quinas pol{\'{i}}ticas como antigamente: competi{\c{c}}{\~{a}}o vertical e mudan{\c{c}}a eleitoral nos estados brasileiros}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0104-44782010000100011{\&}lng=pt{\&}nrm=iso{\&}tlng=pt},
volume = {18},
