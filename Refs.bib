journal = {Econometric Society Monographs},
keywords = {common knowledge,coordination,currency crises,games,global,higher order beliefs,unique equilibrium},
pages = {56----114},
title = {{Global Games: Theory and Applications}},
volume = {35},
year = {2003}
}
