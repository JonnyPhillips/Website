@article{Samuels2000a,
abstract = {Although federalism is a potentially important variable in democratization, few studies explore its impact in democratic transitions and consolidation. Scholars generally agree that federalism is quite strong in contemporary Brazil. This study examines how and why strong federalism reemerged in Brazil following twenty years of centralizing military rule. In brief, the 1964-1985 military regime tried but failed to transform the state-based organizational structure and power base of Brazil's traditional political elite; Brazil's "transitional" electoral cycle also reinforced the strength of state governors. Examples are provided of how subnational actors influenced the transition process in the national government and how state-based actors and interests challenge Brazil's efforts to consolidate its democracy.},
author = {Samuels, David and Abrucio, Fernando Luiz},
