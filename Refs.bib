@article{Shneider2009,
abstract = {Engineering tools and mathematical optimization are applied in this study to plan the work of the agents of the cow artificial insemination service (inseminator) in Israel. Time is crucial in insemination as the chances of conception decline with increasing delay between the start of estrus and insemination. About 1,090 artificial inseminations of cows are performed daily in Israel. They involve 412 farms in 283 villages, and are performed by 29 inseminators; the work plan should balance the work load among the inseminators. To this end, the working time of an inseminator in each village is required. Thus, a model to predict the working time in a village was developed. Subsequently, a mathematical optimization model was designed and solved, which aims to allocate customers to trips and to determine the itinerary of each trip to minimize total distance/time. The main benefits included a 21.4$\backslash${\%} reduction in total traveling time and a 55$\backslash${\%} reduction in the difference between the lengths of the longest and shortest working days. Moreover, the longest delay in reaching an estrous cow is reduced from 7.6 to 5.9 h (i.e., by 1.7 h), which may increase the conception ratio by some 7$\backslash${\%}. In addition, the trade-off between work balance and total traveling time was studied.},
author = {Shneider, B and {Eben Chaime}, M and Gilad, D and Halachmi, I},
doi = {10.3168/jds.2008-1697},
issn = {1525-3198},
journal = {Journal of dairy science},
keywords = {Animal Husbandry,Animal Husbandry: manpower,Animal Husbandry: methods,Animals,Artificial,Artificial: methods,Artificial: veterinary,Biological,Cattle,Female,Humans,Insemination,Israel,Models,Pregnancy,Regression Analysis,Reproducibility of Results,Time Factors,Workload},
month = {may},
number = {5},
pages = {2306--2316},
pmid = {19389989},
title = {{Mathematical optimization to improve cows' artificial insemination services.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3169519{\&}tool=pmcentrez{\&}rendertype=abstract http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3169519{\%}5C{\&}tool=pmcentrez{\%}5C{\&}rendertype=abstract},
volume = {92},
year = {2009}
}
