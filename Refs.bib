pages = {1301--1327},
title = {{The Enduring Indispensability of the Controlled Comparison}},
url = {http://cps.sagepub.com.ezproxy.library.wisc.edu/content/46/10/1301 http://cps.sagepub.com/cgi/doi/10.1177/0010414012472469},
