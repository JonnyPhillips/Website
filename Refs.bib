isbn = {978-0521016452},
issn = {1098-6596},
journal = {Comparative Historical Analysis in the Social Sciences},
pages = {208--240},
pmid = {25246403},
title = {{How institutions evolve. Insights from comparative historical analysis}},
url = {http://catdir.loc.gov/catdir/samples/cam051/2004040785.pdf},
year = {2004}
